{
  "name": "bitmotion/custom-error-page",
  "description": "Shows custom 403/404/503 page depending on domain/language/current tree...",
  "keywords": [
    "Error Handling",
    "403",
    "404",
    "503"
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "homepage": "https://bitmotion.de",
  "authors": [
    {
      "name": "Bitmotion GmbH",
      "email": "typo3-ext@bitmotion.de",
      "role": "Developer",
      "homepage": "https://bitmotion.de/"
    }
  ],
  "support": {
    "issues": "https://github.com/bitmotion/custom-error-page/issues",
    "source": "https://github.com/bitmotion/custom-error-page",
    "email": "typo3-ext@bitmotion.de"
  },
  "require": {
    "typo3/cms-core": "^8.7",
    "symfony/yaml": "^3.0 || ^4.0"
  },
  "conflict": {
    "bitmotion/bm-custom-404-page": "*"
  },
  "autoload": {
    "psr-4": {
      "Bitmotion\\CustomErrorPage\\": "Classes"
    }
  },
  "replace": {
    "bitmotion/custom_error_page": "self.version",
    "typo3-ter/custom-error-page": "self.version"
  }
}
